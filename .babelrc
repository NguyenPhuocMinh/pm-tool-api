{
  "plugins": [
    "source-map-support",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@conf": "./src/conf",
          "@configs": "./src/configs",
          "@constants": "./src/constants",
          "@commons": "./src/commons",
          "@core": "./src/core",
          "@database": "./src/database",
          "@helpers": "./src/helpers",
          "@mappings": "./src/mappings",
          "@middleware": "./src/middleware",
          "@services": "./src/services",
          "@shares": "./src/shares",
          "@utils": "./src/utils",
          "@stores": "./src/stores",
          "@layers": "./src/layers",
          "@adapters": "./src/adapters",
          "@models": "./src/models",
          "@policies": "./src/policies",
          "@routers": "./src/routers",
          "@validators": "./src/validators",
          "@workers": "./src/workers",
          "@transfers": "./src/transfers",
          "@schemas": "./src/schemas",
          "@builds": "./src/builds",
          "@resources": "./src/resources",
          "@data": "./src/data"
        }
      }
    ]
  ],
  "presets": ["@babel/preset-env"]
}
