{
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/api",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/404.js"
    }
  ]
}
